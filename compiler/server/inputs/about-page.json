                            "props": {
                              "name": "Maya Rodriguez",
                              "title": "UX Researcher",
                              "bio": "Understanding users to create better products. Expert in user research and behavioral psychology.",
                              "avatarUrl": "https://i.pravatar.cc/200?img=5",
                              "variant": "glass"
                            }
                          },
                          {
                            "id": "profile-4",
                            "type": "ProfileCard",
                            "props": {
                              "name": "Jordan Smith",
                              "title": "Creative Director",
                              "bio": "Crafting visual stories that inspire. Leading creative strategy and brand development initiatives.",
                              "avatarUrl": "https://i.pravatar.cc/200?img=15",
                              "variant": "glass"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "id": "process-section",
                      "type": "Box",
                      "props": {
                        "style": {
                          "maxWidth": "900px",
                          "margin": "0 auto"
                        }
                      },
                      "slots": {
                        "default": [
                          {
                            "id": "process-title",
                            "type": "GradientText",
                            "props": {
                              "content": "Our Process",
                              "animated": true,
                              "style": {
                                "fontSize": "48px",
                                "fontWeight": "700",
                                "textAlign": "center",
                                "marginBottom": "3rem"
                              }
                            }
                          },
                          {
                            "id": "process-stepper",
                            "type": "Stepper",
                            "props": {
                              "steps": [
                                {
                                  "title": "Discovery",
                                  "description": "Understanding your vision and requirements",
                                  "icon": "üîç"
                                },
                                {
                                  "title": "Design",
                                  "description": "Creating beautiful and functional interfaces",
                                  "icon": "‚úèÔ∏è"
                                },
                                {
                                  "title": "Development",
                                  "description": "Building robust and scalable solutions",
                                  "icon": "üíª"
                                },
                                {
                                  "title": "Launch",
                                  "description": "Deploying and monitoring your success",
                                  "icon": "üöÄ"
                                }
                              ],
                              "currentStep": {
                                "type": "stateBinding",
                                "stateKey": "currentStep"
                              },
                              "showIcons": true,
                              "completedColor": "#10b981",
                              "activeColor": "#667eea",
                              "variant": "horizontal",
                              "style": {
                                "padding": "3rem",
                                "background": "rgba(26, 26, 26, 0.6)",
                                "borderRadius": "20px",
                                "backdropFilter": "blur(10px)"
                              }
                            }
                          },
                          {
                            "id": "stepper-controls",
                            "type": "Box",
                            "props": {
                              "style": {
                                "display": "flex",
                                "justifyContent": "center",
                                "gap": "1rem",
                                "marginTop": "2rem"
                              }
                            },
                            "slots": {
                              "default": [
                                {
                                  "id": "step-prev",
                                  "type": "Button",
                                  "props": {
                                    "style": {
                                      "padding": "0.75rem 2rem",
                                      "fontSize": "16px",
                                      "fontWeight": "600",
                                      "background": "#333",
                                      "color": "white",
                                      "border": "1px solid #555",
                                      "borderRadius": "50px",
                                      "cursor": "pointer",
                                      "transition": "all 0.3s ease"
                                    }
                                  },
                                  "events": {
                                    "click": [
                                      {
                                        "type": "action:setState",
                                        "stateKey": "currentStep",
                                        "newValue": {
                                          "type": "expression",
                                          "value": "Math.max(0, currentStep - 1)"
                                        }
                                      }
                                    ]
                                  },
                                  "slots": {
                                    "default": [
                                      {
                                        "id": "prev-text",
                                        "type": "Text",
                                        "props": {
                                          "content": "‚Üê Previous"
                                        }
                                      }
                                    ]
                                  }
                                },
                                {
                                  "id": "step-next",
                                  "type": "Button",
                                  "props": {
                                    "style": {
                                      "padding": "0.75rem 2rem",
                                      "fontSize": "16px",
                                      "fontWeight": "600",
                                      "background": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
                                      "color": "white",
                                      "border": "none",
                                      "borderRadius": "50px",
                                      "cursor": "pointer",
                                      "boxShadow": "0 6px 20px rgba(102, 126, 234, 0.4)",
                                      "transition": "all 0.3s ease"
                                    }
                                  },
                                  "events": {
                                    "click": [
                                      {
                                        "type": "action:setState",
                                        "stateKey": "currentStep",
                                        "newValue": {
                                          "type": "expression",
                                          "value": "Math.min(3, currentStep + 1)"
                                        }
                                      }
                                    ]
                                  },
                                  "slots": {
                                    "default": [
                                      {
                                        "id": "next-text",
                                        "type": "Text",
                                        "props": {
                                          "content": "Next ‚Üí"
                                        }
                                      }
                                    ]
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
