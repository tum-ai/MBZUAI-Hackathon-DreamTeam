{
  "state": {
    "faqOpen": {
      "type": "boolean",
      "defaultValue": false
    }
  },
  "tree": {
    "id": "page-root",
    "type": "Box",
    "props": {
      "style": {
        "maxWidth": "1200px",
        "margin": "0 auto",
        "padding": "2rem"
      }
    },
    "slots": {
      "default": [
        {
          "id": "hero-title",
          "type": "GradientText",
          "props": {
            "content": "Welcome to the Future",
            "as": "h1",
            "variant": "sunset",
            "style": {
              "fontSize": "72px",
              "textAlign": "center",
              "marginBottom": "3rem"
            }
          }
        },
        {
          "id": "features-section",
          "type": "Box",
          "props": {
            "style": {
              "marginTop": "4rem"
            }
          },
          "slots": {
            "default": [
              {
                "id": "features-heading",
                "type": "Text",
                "props": {
                  "content": "Our Features",
                  "as": "h2",
                  "style": {
                    "fontSize": "36px",
                    "marginBottom": "2rem"
                  }
                }
              },
              {
                "id": "features-grid",
                "type": "Box",
                "props": {
                  "style": {
                    "display": "grid",
                    "gridTemplateColumns": "repeat(3, 1fr)",
                    "gap": "2rem"
                  }
                },
                "slots": {
                  "default": [
                    {
                      "id": "feature-performance",
                      "type": "Card",
                      "props": {
                        "variant": "elevated"
                      },
                      "slots": {
                        "default": [
                          {
                            "id": "perf-title",
                            "type": "Text",
                            "props": {
                              "content": "âš¡ Lightning Fast",
                              "as": "h3",
                              "style": {
                                "fontSize": "24px",
                                "marginBottom": "1rem"
                              }
                            }
                          },
                          {
                            "id": "perf-desc",
                            "type": "Text",
                            "props": {
                              "content": "Optimized for maximum performance and speed"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "id": "feature-design",
                      "type": "Card",
                      "props": {
                        "variant": "elevated"
                      },
                      "slots": {
                        "default": [
                          {
                            "id": "design-title",
                            "type": "Text",
                            "props": {
                              "content": "ðŸŽ¨ Beautiful Design",
                              "as": "h3",
                              "style": {
                                "fontSize": "24px",
                                "marginBottom": "1rem"
                              }
                            }
                          },
                          {
                            "id": "design-desc",
                            "type": "Text",
                            "props": {
                              "content": "Stunning visuals that capture attention"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "id": "feature-easy",
                      "type": "Card",
                      "props": {
                        "variant": "elevated"
                      },
                      "slots": {
                        "default": [
                          {
                            "id": "easy-title",
                            "type": "Text",
                            "props": {
                              "content": "ðŸš€ Easy to Use",
                              "as": "h3",
                              "style": {
                                "fontSize": "24px",
                                "marginBottom": "1rem"
                              }
                            }
                          },
                          {
                            "id": "easy-desc",
                            "type": "Text",
                            "props": {
                              "content": "Intuitive interface anyone can master"
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "id": "tech-list",
          "type": "Box",
          "props": {
            "style": {
              "marginTop": "4rem"
            }
          },
          "slots": {
            "default": [
              {
                "id": "tech-heading",
                "type": "Text",
                "props": {
                  "content": "Technologies We Use",
                  "as": "h2",
                  "style": {
                    "fontSize": "36px",
                    "marginBottom": "2rem"
                  }
                }
              },
              {
                "id": "tech-items",
                "type": "List",
                "props": {
                  "items": [
                    "Vue.js 3 - Modern reactive framework",
                    "TypeScript - Type-safe development",
                    "Vite - Lightning fast build tool",
                    "Tailwind CSS - Utility-first styling"
                  ],
                  "style": {
                    "fontSize": "18px",
                    "lineHeight": "2"
                  }
                }
              }
            ]
          }
        },
        {
          "id": "faq-section",
          "type": "Box",
          "props": {
            "style": {
              "marginTop": "4rem"
            }
          },
          "slots": {
            "default": [
              {
                "id": "faq-heading",
                "type": "Text",
                "props": {
                  "content": "Frequently Asked Questions",
                  "as": "h2",
                  "style": {
                    "fontSize": "36px",
                    "marginBottom": "2rem"
                  }
                }
              },
              {
                "id": "faq-item-1",
                "type": "Accordion",
                "props": {
                  "title": "What is this project about?",
                  "isOpen": {
                    "type": "stateBinding",
                    "stateKey": "faqOpen"
                  }
                },
                "slots": {
                  "default": [
                    {
                      "id": "faq-answer",
                      "type": "Text",
                      "props": {
                        "content": "This is a demonstration of our enhanced component system with semantic IDs, variants, and advanced features."
                      }
                    }
                  ]
                },
                "events": {
                  "click": [
                    {
                      "type": "action:setState",
                      "stateKey": "faqOpen",
                      "newValue": {
                        "type": "expression",
                        "value": "!${state.faqOpen}"
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  }
}
