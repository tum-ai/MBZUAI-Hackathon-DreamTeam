{
  "componentName": "div",
  "friendlyName": "DarkVeil",
  "description": "Dark overlay with gradient veil effect, useful for creating depth and focus",
  "props": {
    "style": {
      "type": "object",
      "description": "Custom CSS styles"
    },
    "class": {
      "type": "string",
      "description": "CSS class name"
    },
    "id": {
      "type": "string",
      "description": "Unique identifier"
    },
    "opacity": {
      "type": "number",
      "default": 0.5,
      "description": "Veil opacity (0-1)"
    },
    "direction": {
      "type": "string",
      "enum": ["top", "bottom", "left", "right", "radial"],
      "default": "bottom",
      "description": "Gradient direction"
    },
    "variant": {
      "type": "string",
      "enum": ["default", "subtle", "strong", "center-focus", "edge-fade"],
      "default": "default",
      "description": "Veil style variant"
    },
    "color": {
      "type": "string",
      "default": "#000000",
      "description": "Base color of veil"
    },
    "blur": {
      "type": "boolean",
      "default": false,
      "description": "Add backdrop blur effect"
    }
  },
  "slots": ["default"],
  "events": [],
  "variants": {
    "default": {
      "description": "Standard dark veil overlay",
      "props": {
        "opacity": 0.5,
        "direction": "bottom",
        "blur": false
      }
    },
    "subtle": {
      "description": "Light veil overlay",
      "props": {
        "opacity": 0.3,
        "direction": "bottom",
        "blur": false
      }
    },
    "strong": {
      "description": "Heavy dark overlay",
      "props": {
        "opacity": 0.8,
        "direction": "bottom",
        "blur": false
      }
    },
    "center-focus": {
      "description": "Radial veil focusing attention to center",
      "props": {
        "opacity": 0.6,
        "direction": "radial",
        "blur": false
      }
    },
    "edge-fade": {
      "description": "Veil with blurred edges",
      "props": {
        "opacity": 0.5,
        "direction": "radial",
        "blur": true
      }
    }
  }
}
