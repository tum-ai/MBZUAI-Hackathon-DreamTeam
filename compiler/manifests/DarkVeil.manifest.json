{
  "componentName": "DarkVeil",
  "friendlyName": "DarkVeil",
  "description": "Dark animated veil effect (Vue Bits component)",
  "requiresImport": true,
  "importPath": "vue-bits",
  "dependencies": ["ogl"],
  "props": {
    "hueShift": {
      "type": "number",
      "default": 0,
      "description": "Shifts the hue of the entire animation"
    },
    "noiseIntensity": {
      "type": "number",
      "default": 0,
      "description": "Intensity of the noise/grain effect"
    },
    "scanlineIntensity": {
      "type": "number",
      "default": 0,
      "description": "Intensity of the scanline effect"
    },
    "speed": {
      "type": "number",
      "default": 0.5,
      "description": "Speed of the animation"
    },
    "scanlineFrequency": {
      "type": "number",
      "default": 0,
      "description": "Frequency of the scanlines"
    },
    "warpAmount": {
      "type": "number",
      "default": 0,
      "description": "Amount of warp distortion applied to the effect"
    },
    "resolutionScale": {
      "type": "number",
      "default": 1,
      "description": "Scale factor for the resolution"
    }
  },
  "slots": [],
  "events": []
}
