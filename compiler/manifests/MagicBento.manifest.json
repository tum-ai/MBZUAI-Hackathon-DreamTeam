{
  "componentName": "MagicBento",
  "friendlyName": "MagicBento",
  "description": "Magic Bento grid container with interactive effects (Vue Bits component)",
  "requiresImport": true,
  "importPath": "vue-bits",
  "dependencies": ["gsap"],
  "props": {
    "enableStars": {
      "type": "boolean",
      "default": true,
      "description": "Enable star particle effects"
    },
    "enableSpotlight": {
      "type": "boolean",
      "default": true,
      "description": "Enable spotlight effect on hover"
    },
    "spotlightRadius": {
      "type": "number",
      "default": 300,
      "description": "Radius of spotlight effect in pixels"
    },
    "enableTilt": {
      "type": "boolean",
      "default": true,
      "description": "Enable 3D tilt effect on hover"
    },
    "clickEffect": {
      "type": "string",
      "enum": ["ripple", "none", "highlight"],
      "default": "ripple",
      "description": "Effect type when clicking"
    },
    "enableMagnetism": {
      "type": "boolean",
      "default": false,
      "description": "Enable magnetic attraction effect"
    },
    "magnetismDistance": {
      "type": "number",
      "default": 100,
      "description": "Distance for magnetic effect in pixels"
    }
  },
  "slots": ["default"],
  "events": []
}
