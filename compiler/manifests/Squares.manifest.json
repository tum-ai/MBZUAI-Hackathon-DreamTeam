{
  "componentName": "Squares",
  "friendlyName": "Squares",
  "description": "Animated squares background (Vue Bits component)",
  "requiresImport": true,
  "importPath": "vue-bits",
  "props": {
    "direction": {
      "type": "string",
      "enum": ["diagonal", "up", "right", "down", "left"],
      "default": "right",
      "description": "Direction of square animation"
    },
    "speed": {
      "type": "number",
      "default": 1,
      "description": "Animation speed multiplier"
    },
    "borderColor": {
      "type": "string",
      "default": "#999",
      "description": "Color of the square borders"
    },
    "squareSize": {
      "type": "number",
      "default": 40,
      "description": "Size of individual squares in pixels"
    },
    "hoverFillColor": {
      "type": "string",
      "default": "#222",
      "description": "Fill color when hovering over squares"
    }
  },
  "slots": [],
  "events": []
}
