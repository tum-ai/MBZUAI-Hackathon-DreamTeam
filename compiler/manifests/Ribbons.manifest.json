{
  "componentName": "Ribbons",
  "friendlyName": "Ribbons",
  "description": "Animated ribbon background effect (Vue Bits component)",
  "requiresImport": true,
  "importPath": "vue-bits",
  "dependencies": ["ogl"],
  "props": {
    "colors": {
      "type": "array",
      "items": {"type": "string"},
      "required": true,
      "description": "Array of color hex codes for ribbons"
    },
    "baseThickness": {
      "type": "number",
      "default": 30,
      "description": "Base thickness of ribbons in pixels"
    },
    "speedMultiplier": {
      "type": "number",
      "default": 0.5,
      "description": "Speed multiplier for ribbon animation"
    },
    "maxAge": {
      "type": "number",
      "default": 500,
      "description": "Maximum age of ribbon particles"
    },
    "enableFade": {
      "type": "boolean",
      "default": true,
      "description": "Enable fade effect on ribbons"
    },
    "enableShaderEffect": {
      "type": "boolean",
      "default": true,
      "description": "Enable WebGL shader effects"
    }
  },
  "slots": [],
  "events": []
}
